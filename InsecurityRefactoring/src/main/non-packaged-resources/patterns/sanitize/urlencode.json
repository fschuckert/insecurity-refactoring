{
	"name":"urlencode",	
	"type":"sanitize",
    
        "pattern_type": "expression",    
        
        "input_type" : "expression",
        "output_type": "expression",
        "output_return":true,
        "passthrough":true,
    
        "data_input":"String",
        "data_output":"String",
    
        "filters": ["allowed_specials:-_.+"],
    
        "code":
        [
            "<call>(urlencode, %input)"
        ]

}
