        "input_type" : "expression",
        "output_type": "expression",
        "output_return":true,
        "passthrough":false,
    
        "data_input":"String",
        "data_output":"Boolean",
    
        "filters": ["specials", "letters"],
    
        "code":
        [
            "<call>(is_real, %input)"
        ]

}
